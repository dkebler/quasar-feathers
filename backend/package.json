{
  "name": "quasar-feathers-app",
  "version": "0.0.1",
  "author": "Your name",
  "description": "Quasar App wrapped with Feathers",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon --watch src --exec npm run debug",
    "predebug": "npm run build",
